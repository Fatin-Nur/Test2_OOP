package pacman;

public abstract class Pacman_abstract implements Characters {
	
	int Pacman_width;
	int Pacman_height;
	int X_pos;
	int Y_pos;
	String Character_direction;
	
		
	public void setX(int X_pos){
		this.X_pos=X_pos;
	}
	public void setY(int Y_pos){
		this.Y_pos=Y_pos;
	}
	public int getX(){
		return this.X_pos;
		}
	public int getY(){
		return this.Y_pos;
		}
	public void eatFood() {
		
	}
	public void Move() {
		
	}

}
